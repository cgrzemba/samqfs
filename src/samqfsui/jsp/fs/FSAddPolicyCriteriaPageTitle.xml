<pagetitle>
  <pagebuttons>
    <cc name="Submit" tagclass="com.sun.web.ui.taglib.html.CCButtonTag">
      <!-- See the CCButton class for button types. -->
      <attribute name="type" value="primary"/>
      <attribute name="dynamic" value="true" />
      <attribute name="defaultValue" value="common.button.submit"/>
      <attribute name="onClick" value="return doSubmit();" />

    </cc>
    <cc name="Cancel" tagclass="com.sun.web.ui.taglib.html.CCButtonTag">
      <!-- See the CCButton class for button types. -->
      <attribute name="type" value="secondary"/>
      <attribute name="defaultValue" value="common.button.cancel"/>
      <attribute name="onClick" value="cleanupPopup(this);"/>
    </cc>
  </pagebuttons>
</pagetitle>
