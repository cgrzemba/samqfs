<table>

<actions>
    <cc name="ButtonEnable"
	tagclass="com.sun.web.ui.taglib.html.CCButtonTag">
        <attribute name="onClick" value="return handleButton(true);"/>
    </cc>
    <cc name="ButtonDisable"
	tagclass="com.sun.web.ui.taglib.html.CCButtonTag">
        <attribute name="onClick" value="return handleButton(false);"/>
    </cc>
</actions>

<column name="ColumnName" sortname="TextName" >
    <cc name="TextName"
        tagclass="com.sun.web.ui.taglib.html.CCStaticTextFieldTag"/>
</column>

<column name="ColumnType" sortname="TextType" extrahtml="align='center'">
    <cc name="TextType"
        tagclass="com.sun.web.ui.taglib.html.CCStaticTextFieldTag"/>
</column>

<column name="ColumnEQ" sortname="TextEQ" >
    <cc name="TextEQ"
        tagclass="com.sun.web.ui.taglib.html.CCStaticTextFieldTag"/>
</column>

<column name="ColumnAlloc" sortname="AllocImage"
    extrahtml="align='center' nowrap='nowrap' valign='center'">
    <cc name="ImageAlloc" tagclass="com.sun.web.ui.taglib.html.CCImageTag" />
</column>

<column name="ColumnCapacity" sortname="usageText"
    extrahtml="align='left' nowrap='nowrap' valign='center'">
    <cc name="ImageUsageBar" tagclass="com.sun.web.ui.taglib.html.CCImageTag">
        <attribute name="hspace" value="20" />
    </cc>
    <cc name="capacityText"
        tagclass="com.sun.web.ui.taglib.html.CCStaticTextFieldTag"/>
    <cc name="usageText" tagclass="com.sun.web.ui.taglib.html.CCHiddenTag"/>
</column>

</table>
