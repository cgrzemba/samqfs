<!-- $Id: FSWizardSharedMemberSelectionTable.xml,v 1.13 2008/05/16 19:39:19 am143972 Exp $ -->

<table>
<column name="SharedMemberCol0">
    <cc name="hostName"
        tagclass="com.sun.web.ui.taglib.html.CCStaticTextFieldTag" />
	<cc name="HiddenHostName" tagclass="com.sun.web.ui.taglib.html.CCHiddenTag" />
	<cc name="HiddenEnabled" tagclass="com.sun.web.ui.taglib.html.CCHiddenTag" />
</column>

<column name="SharedMemberCol1">
    <cc name="samVersion"
        tagclass="com.sun.web.ui.taglib.html.CCStaticTextFieldTag" />
</column>

<column name="SharedMemberCol2">
    <cc name="hardwareArch"
        tagclass="com.sun.web.ui.taglib.html.CCStaticTextFieldTag" />
</column>

<column name="SharedMemberCol3">
    <cc name="clientTextField"
        tagclass="com.sun.web.ui.taglib.html.CCCheckBoxTag">
        <attribute name="checkedValue" value="true" />
        <attribute name="onClick" value="toggleOthers(this)" />
        <attribute name="dynamic" value="true" />
    </cc>
</column>


<column name="SharedMemberCol4">
    <cc name="potentialMetadataServerTextField"
        tagclass="com.sun.web.ui.taglib.html.CCCheckBoxTag">
        <attribute name="checkedValue" value="true" />
        <attribute name="onClick" value="toggleOthers(this)" />
        <attribute name="dynamic" value="true" />
    </cc>
</column>

<column name="SharedMemberCol5">
    <cc name="primaryIPTextField"
        tagclass="com.sun.web.ui.taglib.html.CCDropDownMenuTag">
        <attribute name="dynamic" value="true" />
        <attribute name="disabled" value="true" />
        <attribute name="onChange"
            value="populateTableSecondaryIPDropDown(this)" />
    </cc>
</column>

<column name="SharedMemberCol6">
    <cc name="secondaryIPTextField"
        tagclass="com.sun.web.ui.taglib.html.CCDropDownMenuTag">
        <attribute name="dynamic" value="true" />
        <attribute name="disabled" value="true" />
    </cc>
</column>



</table>
