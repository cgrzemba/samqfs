        tagclass="com.sun.web.ui.taglib.html.CCCheckBoxTag">
        <attribute name="label" value="archiving.diskvsn.flags.unknown"/>
        <attribute name="disabled" value="true"/>
    </cc>
</property>

<property span="true">
    <cc name="remote"
        tagclass="com.sun.web.ui.taglib.html.CCCheckBoxTag">
        <attribute name="label" value="archiving.diskvsn.flags.remote"/>
        <attribute name="disabled" value="true"/>
    </cc>
</property>

