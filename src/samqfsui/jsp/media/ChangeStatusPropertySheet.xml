<!-- $Id: ChangeStatusPropertySheet.xml,v 1.10 2008/03/17 14:40:36 am143972 Exp $ -->

<propertysheet>
    <section name="changestatus" defaultValue="">

    <property span="true">
        <label name="currentLabel" defaultValue="ChangeStatus.psheet1"
                Id="currentId" labelFor="currentValue"/>
        <cc name="currentValue"
                tagclass="com.sun.web.ui.taglib.html.CCStaticTextFieldTag" />
    </property>

    <property>
        <cc name="spacer"
                tagclass="com.sun.web.ui.taglib.spacer.CCSpacerTag" >
          <attribute name="width" value="5"/>
          <attribute name="height" value="3"/>
        </cc>
    </property>

    <property span="true">
        <label name="newLabel" defaultValue="ChangeStatus.psheet2"
                Id="newId" labelFor="newValue"/>
        <cc name="newValue"
                tagclass="com.sun.web.ui.taglib.html.CCDropDownMenuTag" >
        <option label="ChangeStatus.choice1" value="0" />
        <option label="ChangeStatus.choice2" value="1" />
        <option label="ChangeStatus.choice4" value="3" />
        </cc>
    </property>

    </section>
</propertysheet>
