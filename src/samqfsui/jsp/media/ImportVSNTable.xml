            tagclass="com.sun.web.ui.taglib.html.CCStaticTextFieldTag"/>
    </column>

    <column name="TypeColumn" sortname="TypeText" extrahtml="align='center'">
        <cc name="TypeText"
            tagclass="com.sun.web.ui.taglib.html.CCStaticTextFieldTag"/>
