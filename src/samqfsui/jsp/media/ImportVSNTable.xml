<table>
    <actions>
        <cc name="ImportButton"
            tagclass="com.sun.web.ui.taglib.html.CCButtonTag">
            <attribute name="dynamic" value="true" />
            <attribute name="disabled" value="true" />
            <attribute name="defaultValue"
                value="ImportVSN.table.button.import" />
            <attribute name="onClick"
                value="return preSubmitHandler(this);"/>
        </cc>
    </actions>

    <column name="IDColumn" sortname="IDText" extrahtml="nowrap">
        <cc name="IDText"
            tagclass="com.sun.web.ui.taglib.html.CCStaticTextFieldTag"/>
    </column>

    <column name="ACSColumn" sortname="ACSText">
        <cc name="ACSText"
            tagclass="com.sun.web.ui.taglib.html.CCStaticTextFieldTag"/>
    </column>

    <column name="LSMColumn" sortname="LSMText">
        <cc name="LSMText"
            tagclass="com.sun.web.ui.taglib.html.CCStaticTextFieldTag"/>
    </column>
    
    <column name="PanelColumn" sortname="PanelText">
        <cc name="PanelText"
            tagclass="com.sun.web.ui.taglib.html.CCStaticTextFieldTag"/>
    </column>
    
    <column name="RowColumn" sortname="RowText">
        <cc name="RowText"
            tagclass="com.sun.web.ui.taglib.html.CCStaticTextFieldTag"/>
    </column>
    
    <column name="ColumnColumn" sortname="ColumnText">
        <cc name="ColumnText"
            tagclass="com.sun.web.ui.taglib.html.CCStaticTextFieldTag"/>
