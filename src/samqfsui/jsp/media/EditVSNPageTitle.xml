<pagetitle>
    <pagebuttons>
        <cc name="Submit"
            tagclass="com.sun.web.ui.taglib.html.CCButtonTag">
            <!-- See the CCButton class for button types. -->
            <attribute name="type" value="primary"/>
            <attribute name="defaultValue" value="EditVSN.pagebutton1" />
        </cc>

        <cc name="Reset"
            tagclass="com.sun.web.ui.taglib.html.CCButtonTag">
            <!-- See the CCButton class for button types. -->
            <attribute name="type" value="secondary"/>
            <attribute name="defaultValue" value="EditVSN.pagebutton2" />
            <attribute name="onClick"
                value="document.EditVSNForm.reset(); return false;"/>
        </cc>

        <cc name="Cancel"
            tagclass="com.sun.web.ui.taglib.html.CCButtonTag">
            <!-- See the CCButton class for button types. -->
            <attribute name="type" value="secondary"/>
            <attribute name="defaultValue" value="EditVSN.pagebutton3" />
            <attribute name="onClick" value="cleanupPopup(this);"/>
        </cc>
    </pagebuttons>
</pagetitle>
