<!-- Managed bean descriptions for console applications
     that want to use the default bean implementations. 
     If you want to supply alternative data in lieu of 
     supplying it in app.xml you can do so by supplying 
     managed-property entries for the properties you 
     want to change, In that case use a separate 
     faces configuration file and do not include this one
     in your application -->

<faces-config>

    <managed-bean>
        <description>
	    Backing bean for the console masthead.
	    In order to override the default properties of this
	    bean custom values should be supplied as managed properties. 
	</description>
        <managed-bean-name>mastheadBean</managed-bean-name>
        <managed-bean-class>com.sun.web.common.ConsoleMastheadBean
            </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <description>
	    Backing bean for the console version page.
	    In order to override the default properties of this
	    bean custom values should be supplied managed property
	    values. 
	</description>
        <managed-bean-name>versionBean</managed-bean-name>
        <managed-bean-class>com.sun.web.common.ConsoleVersionBean
            </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <description>
	    Backing bean for the console help.
	    In order to override the default properties of this
	    bean custom values should be supplied as
	    managed property values.
	</description>
        <managed-bean-name>JavaHelpBean</managed-bean-name>
        <managed-bean-class>com.sun.web.common.ConsoleHelpBean
            </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

</faces-config>
